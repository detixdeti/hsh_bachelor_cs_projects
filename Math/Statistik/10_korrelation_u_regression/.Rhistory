x <- c(1, 2, 2, 3, 3)
y <- c(4, 6, 3, 5, 2)
plot(x, y)
r.xy <- cor(x, y)
rs.xy <- cor(x, y, method="spearman")
linReg <- lm(y ~ x)
abline(linReg)
linReg2 <- lm(y ~ x + I(x^2))
xSeq = seq(1, 6, length=100)
lines(xSeq, predict(linReg2, data.frame(x=xSeq)))
